{
  "name": "product-variations-view-react",
  "version": "1.0.0",
  "description": "ReactJS-powered settings and options for the Product Variations View plugin.",
  "main": "app.js",
  "devDependencies": {
    "@babel/core": "^7.26.0",
    "@babel/plugin-proposal-class-properties": "^7.18.6",
    "@babel/preset-env": "^7.26.0",
    "@babel/preset-react": "^7.25.9",
    "@types/jquery": "^3.5.32",
    "babel-loader": "^9.2.1",
    "cross-env": "^7.0.3",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "ts-loader": "^9.5.2",
    "typescript": "^5.7.3",
    "webpack": "^5.88.0",
    "webpack-cli": "^4.10.0"
  },
  "scripts": {
    "dev": "cross-env NODE_ENV=development webpack --mode development",
    "build": "cross-env NODE_ENV=production webpack --mode production",

    "dev:settings": "cross-env NODE_ENV=development ENTRY=settings webpack",
    "build:settings": "cross-env NODE_ENV=production ENTRY=settings webpack",
    "build:add-variation-images": "cross-env NODE_ENV=production ENTRY=dro-pvvp-add-variation-images webpack",
    "dev:add-variation-images": "cross-env NODE_ENV=development ENTRY=dro-pvvp-add-variation-images webpack",
    
    
    "build:both": "npm run build && npm run build:unminified",
    "build:unminified": "cross-env NODE_ENV=development webpack --mode development --output-filename [name].js"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/younes-dro/product-variations-view-react.git"
  },
  "keywords": [
    "wordpress",
    "ReactJS",
    "WooCommerce",
    "Plugin"
  ],
  "author": "Younes DRO",
  "license": "GPL-3.0-or-later",
  "bugs": {
    "url": "https://github.com/younes-dro/product-variations-view-react/issues"
  },
  "homepage": "https://github.com/younes-dro/product-variations-view-react#readme",
  "dependencies": {
    "@emotion/react": "^11.13.5",
    "@emotion/styled": "^11.13.5",
    "@mui/material": "^6.1.10"
  }
}
